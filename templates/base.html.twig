<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}To-Do{% endblock %}</title>
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
            <script src="https://kit.fontawesome.com/e9d204bb6d.js" crossorigin="anonymous"></script>
        {% endblock %}
    </head>
    <body>
        {% block body %} {% endblock %}
    </body>
    {% block bottom_javascript %} {% endblock %}
</html>
